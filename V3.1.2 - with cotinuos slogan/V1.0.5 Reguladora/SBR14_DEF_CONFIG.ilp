(*2*)
(* Network 0 *)
(*YCHSe define el tiempo en segundo para las ejecuciones del bloque de software*)
LD    TRUE
MOVE    DI#15, TimerPoll
(* Network 1 *)
(*YCHTiempo en segundos que está habiliatada la logica de apertura de válvula*)
LD    TRUE
MOVE    60, Time2OpenValve
(* Network 2 *)
(*YCHTiempo en segundos que está habiliatada la logica de cierre de válvula*)
LD    TRUE
MOVE    60, Time2CloseValve
(* Network 3 *)
(*YCHArranco por el modo remoto*)
LD    TRUE
MOVE    102, mReg1934
